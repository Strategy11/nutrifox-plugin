{
  "name": "nutrifox",
  "version": "0.1.0",
  "main": "Gruntfile.js",
  "author": "Pinch of Yum",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@wordpress/eslint-plugin": "3.1.0",
    "autoprefixer": "^9.7.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "grunt": "^1.2.1",
    "grunt-postcss": "^0.9.0",
    "grunt-sass": "^3.1.0",
    "grunt-wp-readme-to-markdown": "^2.0.1",
    "node-sass": "^4.13.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "build": "npm run lint:js && cross-env BABEL_ENV=default webpack",
    "dev": "cross-env BABEL_ENV=default webpack --watch",
    "lint": "npm run lint:js && npm run lint:php",
    "lint:fix": "npm run lint:js:fix && npm run lint:php:fix",
    "lint:js": "cross-env eslint assets/block-editor/*",
    "lint:js:fix": "cross-env eslint --fix assets/block-editor/*",
    "lint:php": "./vendor/bin/phpcs",
    "lint:php:fix": "./vendor/bin/phpcbf",
    "test": "npm run test:php",
    "test:php": "./vendor/bin/phpunit"
  }
}
